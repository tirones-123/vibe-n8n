            "output": {
              "postReceive": [
                null
              ]
            }
          }
        },
        {
          "name": "Get Many",
          "value": "getAll",
          "description": "Retrieve a list of users",
          "action": "Get many users",
          "routing": {
            "request": {
              "method": "POST",
              "url": "/v2/users/extensive",
              "body": {
                "filter": {}
