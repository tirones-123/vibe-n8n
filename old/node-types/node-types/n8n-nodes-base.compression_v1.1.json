      "Binary",
      "Files",
      "File"
    ],
    "resources": {
      "primaryDocumentation": [
        {
          "url": "https://docs.n8n.io/integrations/builtin/core-nodes/n8n-nodes-base.compression/"
        }
      ]
    }
  },
  "name": "n8n-nodes-base.compression"
}